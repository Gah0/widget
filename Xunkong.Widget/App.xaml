            <x:String x:Key="Image_HomeCoin">ms-appx:///Assets/Images/UI_ItemIcon_204.png</x:String>
            <x:String x:Key="Image_Transformer">ms-appx:///Assets/Images/UI_ItemIcon_220021_64.png</x:String>
            <x:String x:Key="Image_QuestEvent">ms-appx:///Assets/Images/UI_NPCTopIcon_QuestEvent_64.png</x:String>
            <x:String x:Key="Image_Primogems">ms-appx:///Assets/Images/UI_ItemIcon_201.png</x:String>
            <x:String x:Key="Image_Mora">ms-appx:///Assets/Images/UI_ItemIcon_202.png</x:String>
            <x:String x:Key="Image_Paimon">ms-appx:///Assets/Images/UI_EmotionIcon5.png</x:String>
            <x:String x:Key="Image_Explore">ms-appx:///Assets/Images/UI_Icon_Intee_Explore_1.png</x:String>



