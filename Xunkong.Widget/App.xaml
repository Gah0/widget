            <xwc:DateTimeOffsetToDayStringConverter x:Key="DateTimeOffsetToDayStringConverter"/>
            <xwc:ExpeditionStatusToColorConverter x:Key="ExpeditionStatusToColorConverter"/>
            <xwc:ObjectToVisibilityConverter x:Key="ObjectToVisibilityConverter"/>
            <xwc:ObjectToVisibilityReversedConverter x:Key="ObjectToVisibilityReversedConverter"/>
            <xwc:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
            <xwc:BoolToVisibilityReversedConverter x:Key="BoolToVisibilityReversedConverter"/>
            <xwc:QuestEventStatsColorConverter x:Key="QuestEventStatsColorConverter"/>
            <xwc:QuestEventStatsColorForXboxWidgetConverter x:Key="QuestEventStatsColorForXboxWidgetConverter"/>
